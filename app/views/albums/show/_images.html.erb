<div class="nf-item <%= photo.user.nickname %> spacing">
  <div class="item-box">
  <a class="cbox-gallery1" href="<%= photo.media_url %>" title="<%= photo.location_name %>">
      <img alt="1" src="<%= photo.media_url %>" class="item-container" data-link="<%= photo.media_url %>" data-lat="<%= photo.latitude %>" data-long="<%= photo.longitude %>" data-name="<%= photo.location_name %>">
      <div class="item-mask">
        <div class="item-caption">
          <h5 class="white"><%= photo.location_name %></h5>
        </div>
      </div>
    </a>
  </div>
</div>